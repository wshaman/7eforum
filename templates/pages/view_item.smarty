<table width="200" border="0">
    <tr>
        <th>Parent category</th>
        <td>{$parent}</td>
    </tr>
    <tr>
        <th>Item name</th>
        <td>{$item.name}</td>
    </tr>

    	
		{foreach from=$fieldlist item=field}
			<tr><th>{$field.name}</th><td>{$field.value}</td></tr>
		{/foreach}		

</table>
